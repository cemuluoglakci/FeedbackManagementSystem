@model ApplicationFMS.Handlers.Feedbacks.Queries.GetPublicFeedbackList.FeedbackListVm


<div class="row match-height">
    @foreach (var feedback in Model.PublicFeedbackList)
    {
        @await Component.InvokeAsync("SingleFeedback", feedback)

    }
</div>

@section Scripts{
    <script>
        $(document).ready(function () {
            $('#viewallfollowers').click(function () {
                {
                    //$("#followersrefresh").empty();
                    var _url = '@Url.Action("TestView", "Feedback")';
                    $.ajax({
                        type: "GET",
                        url: _url,
                        //data: { uid: $(this).prop("name") },
                        success: function (result) {
                            $("#followersrefresh").append(result);
                        },
                    });
                }
            });
        });
    </script>
}
<input id="viewallfollowers" type="button" value="button" name="1"/>
<div id="followersrefresh" class="row match-height">
</div>